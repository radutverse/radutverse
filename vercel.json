{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "dist/spa",
      "use": "@vercel/static"
    },
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api"
    },
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/placeholder.svg",
      "dest": "/placeholder.svg"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
