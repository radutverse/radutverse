{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "dist/spa",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "status": 404
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
